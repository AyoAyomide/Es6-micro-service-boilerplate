{
  "name": "micro-service",
  "version": "0.0.0",
  "description": "micro service boilerplate",
  "private": true,
  "main": "dist",
  "scripts": {
    "clean": "node ./node_modules/rimraf/bin.js dist",
    "compile": "node ./node_modules/@babel/cli/bin/babel.js ./src --out-dir dist",
    "build": "npm run clean & npm run compile",
    "start": "node ./dist/bin/www",
    "dev": "nodemon -w src --exec \"(rimraf dist) & (babel ./src --out-dir dist) & nodemon ./dist/bin/www\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "eslint": "^7.26.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": "12.x"
  }
}