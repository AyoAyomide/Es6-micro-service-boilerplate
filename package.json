{
  "name": "micro-service",
  "version": "0.0.0",
  "description": "micro service boilerplate",
  "private": true,
  "main": "dist",
  "scripts": {
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development npm install --only=dev --dev",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;",
    "prestart": "(rimraf dist) & (babel ./src --out-dir dist)",
    "build":"(rimraf dist) & (babel ./src --out-dir dist)",
    "start": "node ./dist/bin/www",
    "dev": "nodemon -w src --exec \"(rimraf dist) & (babel ./src --out-dir dist) & nodemon ./dist/bin/www\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "eslint": "^7.26.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": "12.x"
  }
}
