{
  "name": "micro-service",
  "version": "0.0.0",
  "private": true,
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "scripts": {
    "prestart": "(rimraf dist) & (babel ./src --out-dir dist)",
    "start": "node ./dist/bin/www",
    "dev": "nodemon -w src --exec \"(rimraf dist) & (babel ./src --out-dir dist) & nodemon ./dist/bin/www\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1"
  }
}
